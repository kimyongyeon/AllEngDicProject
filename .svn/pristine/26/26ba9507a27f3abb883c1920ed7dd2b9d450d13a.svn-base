<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="fill_parent"
  android:layout_height="fill_parent">
  
	<LinearLayout
	    android:background="@drawable/fav_list"
  		android:orientation="horizontal"
  		android:gravity="center_vertical"
  		android:layout_width="fill_parent"
  		android:layout_height="wrap_content">
  	
  	<LinearLayout
  		android:orientation="vertical"
  		android:gravity="left"
  		android:layout_width="fill_parent"
  		android:layout_height="49dp"
  	    android:layout_weight="2">
  	<TextView
  		android:id="@+id/row2Tx01"
  		android:textSize="18dp"
  		android:paddingLeft="10dp"
  		android:layout_gravity="left"
  		android:layout_width="fill_parent"
  		android:layout_height="wrap_content"
  		android:textColor="#464646"
 		android:text="단어"/>
	<TextView
  		android:id="@+id/row2Tx02"
  		android:textSize="12dp"
  		android:paddingLeft="10dp"
  		android:layout_gravity="left"
  		android:layout_width="fill_parent"
  		android:layout_height="wrap_content"
  		android:textColor="#b6b6b6"
 		android:text="날짜"/>
	</LinearLayout>
	
  	<LinearLayout
  		android:layout_width="fill_parent"
  		android:layout_height="wrap_content"
  		android:layout_marginRight="2dp"
  		android:layout_weight="14">
		<ImageButton
			android:id="@+id/row2Bt01"
			android:background="@drawable/popup_list_fav_btn"
			android:layout_width="29.3dp"
			android:layout_height="29.3dp"/>
	</LinearLayout>
	
	</LinearLayout>
	
	<View
		android:layout_width="fill_parent"
	    android:layout_height="1dp"
	/>
	 	
</LinearLayout>
